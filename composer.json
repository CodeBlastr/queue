{
    "name": "codeblastr/queue",
    "description": "Notifiation management plugin for CakePHP 3.x, which stacks messages into a queue for sending via cron.",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "messages",
        "queue",
        "cron",
        "admin"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Richard Kersey",
            "homepage": "http://www.richardkersey.com",
            "role": "Author"
        },
        {
            "name": "Others",
            "homepage": "https://github.com/codeblastr/queue/graphs/contributors"
        }
    ],
    "require": {
        "cakephp/cakephp": "~3.2",
        "phpunit/phpunit": "~4.1"
    },
    "autoload": {
        "psr-4": {
            "CodeBlastrQueue\\": "src",
            "CodeBlastrQueue\\Test\\Fixture\\": "tests\\Fixture"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeBlastrQueue\\Queue\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
